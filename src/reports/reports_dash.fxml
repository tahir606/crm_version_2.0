<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXDatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="800.0" stylesheets="@../styles/reports.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="reports.ReportsDashController">
   <center>
      <AnchorPane fx:id="anchor_center" prefHeight="450.0" prefWidth="677.0" BorderPane.alignment="CENTER">
         <children>
            <HBox fx:id="hbox_filters" layoutX="424.0" layoutY="3.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="29.0" prefWidth="100.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="0.0" />
            <HBox fx:id="hbox_User" layoutX="202.0" layoutY="3.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="29.0" prefWidth="100.0" AnchorPane.rightAnchor="322.0" AnchorPane.topAnchor="0.0" />
            <Label fx:id="txt_reportName" layoutX="14.0" layoutY="6.0" text="Report Name">
               <font>
                  <Font name="Arial Narrow Bold" size="18.0" />
               </font>
            </Label>
            <Label fx:id="txt_Count" layoutX="4.0" layoutY="382.0" prefWidth="143.0" text="Displaying 0 Emails" textFill="#395403" AnchorPane.bottomAnchor="2.0">
               <font>
                  <Font name="Calibri Italic" size="14" />
               </font>
            </Label>
            <HBox fx:id="hbox_DatePicker" layoutX="434.0" layoutY="13.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="37.0" prefWidth="208.0" AnchorPane.rightAnchor="110.0" AnchorPane.topAnchor="0.0">

                  <children>
                     <JFXDatePicker fx:id="from" prefHeight="25.0" prefWidth="100.0" promptText="From" />
                     <JFXDatePicker fx:id="to" prefHeight="25.0" prefWidth="100.0" promptText="To" />
                  </children>

            </HBox>
         </children>
      </AnchorPane>
   </center>
   <left>
      <VBox fx:id="vbox_reports" spacing="10" style="-fx-background-color: #b3efb7;" BorderPane.alignment="CENTER">
         <children>
            <HBox prefHeight="100.0" prefWidth="105.0" />
         </children>
      </VBox>
   </left>
</BorderPane>
